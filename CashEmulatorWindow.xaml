<Window x:Class="VendingMachine.CashEmulatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VendingMachine"
        mc:Ignorable="d"
        Title="Cash Emulator" Height="240" Width="200" ResizeMode="NoResize" Closing="Window_Closing">
    <StackPanel Orientation="Vertical">
        <Button Margin="10" Tag="50" Click="Button_Click">50 RUB</Button>
        <Button Margin="10" Tag="100" Click="Button_Click">100 RUB</Button>
        <Button Margin="10" Tag="500" Click="Button_Click">500 RUB</Button>
        <Button Margin="10" Tag="1000" Click="Button_Click">1000 RUB</Button>
        <Button Margin="10" Tag="5000" Click="Button_Click">5000 RUB</Button>
    </StackPanel>
</Window>
